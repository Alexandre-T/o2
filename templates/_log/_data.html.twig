<dl class="row">
    {% for key, value in data %}
        {% if value is array and value.id is defined %}
            {% if value.id is not empty %}
                <dt class="col-sm-4 text-right">{{ ('entity.log.data.' ~ key | hyphenize ) | trans }}</dt>
                {# FIXME remplacer value #}
                <dd class="col-sm-8">{{ value.id }}</dd>
            {% endif %}
        {% elseif key == 'password' %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.password' | trans }}</dt>
            <dd class="col-sm-8">&bull;&bull;&bull;&bull;&bull;</dd>
        {% elseif key == 'country' %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.country' | trans }}</dt>
            <dd class="col-sm-8">{{ log.data.country | country }}</dd>
        {% elseif key == 'roles' %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.roles' | trans }}</dt>
            <dd class="col-sm-8">{{ log.data.roles | roles }}</dd>
        {% elseif key == 'resettingAt' and value is not empty %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.resetting-at' | trans }}</dt>
            <dd class="col-sm-8">{{ log.data.resettingAt | app_date }}</dd>
        {% elseif key == 'createdAt' and value is not empty %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.created-at' | trans }}</dt>
            <dd class="col-sm-8">{{ log.data.createdAt | app_date }}</dd>
        {% elseif key == 'canceledAt' and value is not empty %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.canceled-at' | trans }}</dt>
            <dd class="col-sm-8">{{ log.data.canceledAt | app_date }}</dd>
        {% elseif key == 'paidAt' and value is not empty %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.paid-at' | trans }}</dt>
            <dd class="col-sm-8">{{ log.data.paidAt | app_date }}</dd>
        {% elseif key == 'type' %}
            <dt class="col-sm-4 text-right">{{ 'entity.log.data.type' | trans }}</dt>
            <dd class="col-sm-8">
                {% if log.data.type is society %}
                    {{ 'entity.log.society-physic' | trans }}
                {% else %}
                    {{ 'entity.log.society-morale' | trans }}
                {% endif %}
            </dd>
        {% elseif not(value is empty and creating) %}
            <dt class="col-sm-4 text-right">{{ ('entity.log.data.' ~ key | hyphenize ) | trans }}</dt>
            <dd class="col-sm-8">{{ value | emptyize }}</dd>
        {% endif %}
    {% endfor %}
</dl>
